  "private": true,
  "engines": {
    "node": ">= 6.0.0",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "node webpack/index | NODE_ENV=development node src/server",
    "build": "yarn run clean && NODE_ENV=production webpack --config webpack/config.prod.withjs.babel.js && rm build/static-*",
    "build:nojs": "yarn run clean && NODE_ENV=production webpack --config webpack/config.prod.nojs.babel.js && rm build/static-* && rm build/app-*.js"
  },
  "dependencies": {
    "@cloudflare/json-schema-apidoc-loader": "^0.1.0",
