    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.1",
